----------7
CREATE TABLE Supplier(
	ID serial PRIMARY KEY,
	Name varchar(20) NOT NULL,
	Phone_No numeric,
	City varchar(20)
);

CREATE TABLE Products(
	ID serial PRIMARY KEY,
	Name varchar(30) NOT NULL UNIQUE,
	Unit_Price int,
	Quantity int,
	Total_Amount int,
	Supplier_ID int REFERENCES Supplier(ID)
);

SELECT * FROM Supplier
SELECT * FROM Products

