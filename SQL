----------6
CREATE TABLE Customers2(
	ID serial PRIMARY KEY,
	Name varchar(30) NOT NULL,
	Phone_No numeric,
	City varchar(15)
);

CREATE TABLE Stock(
	ID serial PRIMARY KEY,
	Product_Name varchar(20) UNIQUE NOT NULL,
	Quantity int,
	Unit_Price int
);

CREATE TABLE Orders2(
	ID serial PRIMARY KEY,
	Customer_ID int REFERENCES Customers2(ID),
	Stock_ID int REFERENCES Stock(ID),
	Total_Amount int,
	Quantity int,
	Order_Date date,
	Placed_Date date
);

SELECT * FROM customers2
SELECT * FROM Stock
SELECT * FROM Orders2

